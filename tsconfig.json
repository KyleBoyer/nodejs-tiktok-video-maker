{
    "compilerOptions": {
        "esModuleInterop": true,
        "module": "commonjs",
        "noImplicitAny": true,
        "outDir": "dist",
        "preserveConstEnums": true,
        "removeComments": true,
        "sourceMap": true,
        "skipLibCheck": true
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules/**/*"]
}